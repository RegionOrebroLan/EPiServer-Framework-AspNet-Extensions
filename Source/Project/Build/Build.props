<Project>
	<PropertyGroup Label="Initial">
		<Company>Region Örebro län</Company>
		<VersionPrefix>0.0.0</VersionPrefix>
		<VersionPrereleaseName>alpha</VersionPrereleaseName>
	</PropertyGroup>
	<PropertyGroup Label="Assembly">
		<AssemblyName>RegionOrebroLan.EPiServer.Framework.AspNet</AssemblyName>
		<AssemblyOriginatorKeyFile>$(SolutionDir).Global\Signing\StrongName.snk</AssemblyOriginatorKeyFile>
		<AssemblyVersion>$(VersionPrefix).0</AssemblyVersion>
		<CodeAnalysisRuleSet>CodeAnalysis\Rules.ruleset</CodeAnalysisRuleSet>
		<DelaySign>false</DelaySign>
		<Description>EPiServer-framework (AspNet) additions and extensions.</Description>
		<FileVersion>$(VersionPrefix).0</FileVersion>
		<RootNamespace>RegionOrebroLan.EPiServer</RootNamespace>
		<SignAssembly>true</SignAssembly>
		<TargetFramework>net461</TargetFramework>
		<Version>$(VersionPrefix)</Version>
		<Version Condition="$(VersionPrereleaseName) != ''">$(Version)-$(VersionPrereleaseName)</Version>
	</PropertyGroup>
	<PropertyGroup Label="NuGet-Package">
		<Authors>$(Company)</Authors>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageProjectUrl>https://github.com/RegionOrebroLan/EPiServer-Framework-AspNet-Extensions/</PackageProjectUrl>
		<PackageTags></PackageTags>
	</PropertyGroup>
</Project>